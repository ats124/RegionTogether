  <metadata>
    <id>RegionTogether</id>
    <version>1.0.0.0</version>
    <title>RegionTogether</title>
    <authors>ats124</authors>
    <owners>ats124</owners>
    <licenseUrl>https://github.com/ats124/RegionTogether/blob/master/LICENSE</licenseUrl>
